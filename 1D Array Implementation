//Write a program to implement array 
#include <stdio.h>
int main()
{
    int arr[100],n,i;
    printf("\nEnter the number of elements:");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
      scanf("%d",&arr[i]);  
    }
    printf("\nPrinting Array elements Below\n");
    for(i=0;i<n;i++)
    {
        
        printf("%d\n",arr[i]);
    }
    return 0;
}
